<script>
    let lastClickedTab = null;

    function handleTabClick(element) {
        const container = document.querySelector(".qure_tabs");

        if (lastClickedTab && lastClickedTab !== element) {
            // Determine the scroll offset to keep the clicked element at the left edge
            const containerLeft = container.getBoundingClientRect().left;
            const elementLeft = element.getBoundingClientRect().left;
            const offset = elementLeft - containerLeft - 10; // Adjust for spacing if needed

            // Scroll horizontally within the container
            container.scrollBy({ left: offset, behavior: "smooth" });
        } else if (!lastClickedTab) {
            // Scroll the clicked tab into view horizontally without moving vertically
            const elementLeft = element.getBoundingClientRect().left;
            const containerLeft = container.getBoundingClientRect().left;
            const horizontalOffset = elementLeft - containerLeft;

            container.scrollBy({ left: horizontalOffset, behavior: "smooth" });
        }

        // Update the last clicked tab
        lastClickedTab = element;
    }

    class ToggleColumns {
        constructor(container) {
            this.container = container;
            this.button = this.container.querySelector('.toggle-columns');

            // Get the number of columns to hide from the data attribute, defaulting to 2
            const columnsToHide = parseInt(this.container.getAttribute('data-hide-columns'), 10) || 2;

            // Select the last specified number of columns
            this.columns = Array.from(this.container.querySelectorAll('.row .col-6')).slice(-columnsToHide);
            this.isExpanded = false;

            this.button.addEventListener('click', () => this.toggleColumns());
        }

        toggleColumns() {
            this.isExpanded = !this.isExpanded;
            this.columns.forEach(column => {
                column.classList.toggle('hidden', !this.isExpanded);
            });
            this.button.textContent = this.isExpanded ? 'See Less' : 'See More';
        }
    }

    document.addEventListener('DOMContentLoaded', () => {
        document.querySelectorAll('.pd_videos-mob').forEach(container => {
            new ToggleColumns(container);
        });
    });

</script>