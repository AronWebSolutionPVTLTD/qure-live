<script>
    document.addEventListener('DOMContentLoaded', () => {
        // IDs of elements to handle click events
        const elementIds = ['hs_1', 'hs_2', 'hs_3'];

        // Function to handle click events
        const handleElementClick = (event) => {
        event.preventDefault(); // Prevent default action (e.g., link navigation)

        const targetElement = event.currentTarget; // The element that triggered the event
        const sectionValue = targetElement.getAttribute('href')?.split('#')[1]; // Extract the value after #

        if (!sectionValue) {
            console.error('Failed to extract value after # in href.');
            return;
        }

        // Find the link with the corresponding data-section and trigger a click
        const targetLink = document.querySelector(`a[data-section="${sectionValue}"]`);
            if (targetLink) {
                console.log(`Clicking link with data-section="${sectionValue}"`);
                targetLink.click();
            } else {
                console.error(`Link with data-section="${sectionValue}" not found.`);
            }
        };

        // Function to check the URL for a hash parameter on page load
        const checkUrlForSection = () => {
        const hash = window.location.hash.slice(1); // Get the value after # in the URL
            if (hash) {
                console.log(`Checking URL hash: "${hash}"`);
                const targetLink = document.querySelector(`a[data-section="${hash}"]`);
                if (targetLink) {
                    console.log(`Clicking link with data-section="${hash}" from URL.`);
                    targetLink.click();
                } else {
                    console.error(`Link with data-section="${hash}" from URL not found.`);
                }
            }
        };

        // Add click event listeners to all elements in the array
        elementIds.forEach((id) => {
            const element = document.getElementById(id);
            if (element) {
                console.log(`Adding click listener to element with id="${id}"`);
                element.addEventListener('click', handleElementClick);
            } else {
                console.warn(`Element with id="${id}" not found.`);
            }
        });

        // Check the URL for a hash parameter when the page loads
        setTimeout(() => {
            checkUrlForSection();
        }, 600);
    });
</script>

<script>
    document.addEventListener('DOMContentLoaded', () => {
        const zeroStep = document.getElementById('zero_step');
        const stepNext = document.getElementById('step_next');
        const topMenuText = document.getElementById('top_menu_text_id');
        const menuItems = document.querySelectorAll('.menu-item');
        const steps = document.querySelectorAll('.step');
        const nextButton = document.querySelector('.next-button');
        const prevButton = document.querySelector('.prev-button');
        const tabText = document.querySelector('.tab-text p'); // The <p> inside .tab-text for displaying the next tab's name

        // Initially show `zero_step` and hide `step_next`
        zeroStep.style.display = 'block';
        stepNext.style.display = 'none';

        // Function to show a specific step
        const showStep = (stepId) => {
            steps.forEach(step => {
                step.classList.toggle('active', step.id === stepId);
                step.classList.toggle('hidden', step.id !== stepId);
            });

            // Add active class to the parent <li> of the menu item
            menuItems.forEach(item => {
                const parentLi = item.closest('li'); // Get the parent <li> element
                parentLi.classList.toggle('active', item.dataset.section === stepId);
            });

            updateButtons(stepId);
        };

        const updateButtons = (currentStepId) => {
            const currentIndex = Array.from(steps).findIndex(step => step.id === currentStepId);

            // Update "Next" button and show next tab name in `tab-text`
            if (currentIndex < steps.length - 1) {
                const nextStep = steps[currentIndex + 1];
                let nextTabName = nextStep.querySelector('h5').textContent.trim();
                nextTabName = nextTabName.replace(/^0(\d)/, '$1'); // Remove leading 0 if it exists
                nextTabName = nextTabName.replace(/^(\d+)/, '$1.'); // Add a dot after the number
                tabText.textContent = ` ${nextTabName}`; // Show next tab's name in tab-text
                nextButton.dataset.next = nextStep.id;
                nextButton.classList.remove('hidden');
                nextButton.classList.remove('visible_hide'); // Ensure it doesn't have visible_hide
            } else {
                const prevStep = steps[currentIndex - 1];
                let prevTabName = prevStep.querySelector('h5').textContent.trim();
                prevTabName = prevTabName.replace(/^0(\d)/, '$1'); // Remove leading 0 if it exists
                prevTabName = prevTabName.replace(/^(\d+)/, '$1.'); // Add a dot after the number
                tabText.textContent = ` ${prevTabName}`;
                nextButton.classList.add('visible_hide');
            }

            // Update "Prev" button
            if (currentIndex > 0) {
                const prevStep = steps[currentIndex - 1];
                prevButton.dataset.prev = prevStep.id; // Store previous step's ID in dataset
            }

            // Remove "visible_hide" from `prevButton` at all times
            prevButton.classList.remove('visible_hide');

            // Add 'btn-close-white' class if on the last step
            if (currentIndex === steps.length - 1) {
                prevButton.classList.add('btn-close-white');
            } else {
                prevButton.classList.remove('btn-close-white');
            }
        };


        // Click event for menu items in `zero_step`
        menuItems.forEach(item => {
            item.addEventListener('click', (e) => {
                e.preventDefault();
                const stepId = item.dataset.section;

                // Hide `zero_step` and show `step_next`
                zeroStep.style.display = 'none';
                stepNext.style.display = 'block';

                // Display the clicked step
                showStep(stepId);
            });
        });

        // Click event for "Next" button
        nextButton.addEventListener('click', () => {
            const nextStepId = nextButton.dataset.next;
            if (nextStepId) {
                showStep(nextStepId);
            }
        });

        // Click event for "Prev" button
        prevButton.addEventListener('click', () => {
            const prevStepId = prevButton.dataset.prev;
            if (prevStepId) {
                showStep(prevStepId); // Navigate to the previous step
            }
        });

        // Function to toggle visibility of `zero_step` and `step_next`
        const toggleSteps = () => {
            zeroStep.style.display = 'block';
            stepNext.style.display = 'none';

            // Reset active steps
            steps.forEach(step => step.classList.add('hidden'));
            menuItems.forEach(item => item.classList.remove('active'));
        };

        // Add click event to the top menu text
        if (topMenuText) {
            topMenuText.addEventListener('click', (e) => {
                e.preventDefault();
                toggleSteps();
            });
        }

        // Add the showTabNavigation function for the "Let's Get Started" button
        const showTabNavigation = () => {
            // Hide zero_step and show step_next
            zeroStep.style.display = 'none';
            stepNext.style.display = 'block';

            // Optionally, show the first tab or do any further setup
            if (menuItems.length > 0) {
                showStep(menuItems[0].dataset.section); // Show the first step
            }
        };

        // Event listener for the "Let's Get Started" button
        const startButton = document.querySelector('.let_start');
        if (startButton) {
            startButton.addEventListener('click', showTabNavigation);
        }

        // Initialize steps: hide all except the first one
        steps.forEach(step => step.classList.add('hidden'));
        if (menuItems.length > 0) {
            showStep(menuItems[0].dataset.section);
        }
    });
</script>