        "column_KXDayz": {
          "type": "column",
          "settings": {
            "heading": "Alleviates many different skin concerns",
            "text": "<p>Choose between an anti-aging treatment, an anti-breakout treatment, or customized treatment modes for each facial area.<\/p>",
            "image": "shopify:\/\/shop_images\/cus_acc_3.webp"
          }
        },
        "column_JATQdF": {
          "type": "column",
          "settings": {
            "heading": "Track your treatments with Qure mobile app",
            "text": "<p>Seamlessly pairs with our mobile app (iPhone and Android) for easy treatment planning, tracking, and scheduling. Never miss a treatment with in-app phone reminders, too.<\/p>",
            "image": "shopify:\/\/shop_images\/cus_acc_4.webp"
          }
        }
      },
      "block_order": [
        "column_7B3jgk",
        "column_ThhqcA",
